{% extends 'Base.html.twig' %}
{% block AjouterEvent %}
    <section id="reservation" class="reservation_section bg-grey">
        <div class="container padding">
            <section id="main-content">
                <section class="wrapper">
                    <div class="row">
                        <div class="col-lg-12 col-sm-offset-0">
                            <section class="panel" id="ABC">
                                <header style="font-size: 20px" class="panel-heading col-sm-offset-0" id="ABCDE">
                                    <strong>Ajouter Un Evenement</strong>
                                </header>
                                <header class="panel-heading">
                        <br>
                        {{ form_start(form) }}
                        {{ form_row(form.nom) }}
                        <br>
                        {{ form_row(form.date) }}
                        <br>
                        {{ form_row(form.description) }}
                        <br>
                        {{ form_row(form.Ajouter,{'label':'Ajouter'}) }}
                        {{ form_end(form) }}
                                </header>
                            </section>
                        </div>
                    </div>
                </section>
            </section>
        </div>
    </section>
{% endblock %}