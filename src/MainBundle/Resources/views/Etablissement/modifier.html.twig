{% extends 'Base.html.twig' %}

{% block AjouterEtablissement %}
    <section id="reservation" class="reservation_section bg-grey">
        <div class="container padding">
            <div class="col-sm-10 col-sm-offset-1 align-left">
                <div class="reserv_form">
                    <div class="section_heading mb-40">
                        {{ form_start(et) }}
                        <table>
                            <tr>
                                <td>
                                    {{ form_label(et.nom) }}
                                </td>
                                <td>
                                    {{ form_errors(et.nom) }}
                                    {{ form_widget(et.nom) }}
                                </td>
                            </tr>
                            <tr>
                                <td>
                                    {{ form_label(et.adresse) }}
                                </td>
                                <td>
                                    {{ form_errors(et.adresse) }}
                                    {{ form_widget(et.adresse) }}
                                </td>
                            </tr>
                            <tr>
                                <td>
                                    {{ form_label(et.description) }}
                                </td>
                                <td>
                                    {{ form_errors(et.description) }}
                                    {{ form_widget(et.description) }}
                                </td>
                            </tr>
                            <tr>
                                <td>
                                    {{ form_label(et.horaire) }}
                                </td>
                                <td>
                                    {{ form_errors(et.horaire) }}
                                    {{ form_widget(et.horaire) }}
                                </td>
                            </tr>
                            <tr>
                                <td>

                                </td>
                                <td>
                                    {{ form_row(et.Ajouter, {'label':'Modifier Etablissement','attr':{'class':'default_btn'}}) }}
                                </td>
                            </tr>
                            <tr>
                                <td>

                                </td>
                                <td>
                                    {{ form_end(et) }}
                                </td>
                            </tr>
                        </table>
                    </div>
                </div>
            </div>
        </div>
        </div>
    </section>
{% endblock %}